<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

	<g:evaluate>
		var today = new GlideDateTime();
		var todaynum = today.getDayOfWeekUTC();
		
		var friday = 5;
		var xd;
		
		if (todaynum == friday) {
		var xd = "FREDAG";
		} else { 
		var xd = "INTE FREDAG";
		}
		
		var words = [xd];
	</g:evaluate>
		
	<j:forEach var="jvar_word" items="${words}">
		<center><h1>Är det fredag?</h1>
			<b><h3>Det är ${jvar_word}</h3></b></center>
	</j:forEach>	
	
</j:jelly>]]></programmatic_content>
